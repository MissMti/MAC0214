#include <bits/stdc++.h>
using namespace std;

long long MAX=1000000007;

signed main() {
    //ios::sync_with_stdio(0);
    //cin.tie(NULL);cout.tie(NULL);

    long long n,a,erro=0;
    cin>>n;
    for(int i=0;i<n;i++){
        cin>>a;
        if(a>10){
            erro+=a-10;
        }
    }
    cout<<erro<<"\n";

}
