#include <bits/stdc++.h>
using namespace std;

long long MAX=1000000007;

signed main() {
    //ios::sync_with_stdio(0);
    //cin.tie(NULL);cout.tie(NULL);

    long long a,b,c,erro=1;
    cin>>a>>b>>c;
    for(int i=a;i<=b;i++){
        if(i%c==0){
            cout<<i<<"\n";
            return 0;
        }

    }
    cout<<"-1\n";

}
